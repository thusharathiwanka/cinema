{
  "root": true,
  "env": {
    "browser": true,
    "jest/globals": true,
    "node": true
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:nuxt/recommended",
    "prettier"
  ],
  "plugins": ["jest"],
  // add your custom rules here
  "rules": {
    "vue/multi-word-component-names": 0,
    "vue/attribute-hyphenation": 0
  }
}
